<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="smax.cls07.proj1.view.Page1"
		 xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Customer List">
		<content>
	       <List items="{/Customers}">
	          <items>
	             <StandardListItem title="{CompanyName}" description="{CustomerID}"
	             type="Active" press="onItemSelection"
	             />
	          </items>
	       </List>
		</content>
	</Page>
</core:View>